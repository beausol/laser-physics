\begin{equation} \label{eqn:mflt_field_ansatz}
    E(T, t) \equiv A\, e^{-\half\, a\, \tau^2 + i\, \phi(T, \tau)}\, ,
\end{equation}
where
\begin{equation}
    \phi(T,\tau) \equiv \lambda\, T - \half\, b\, \tau^2\, .
\end{equation}
If we define
\begin{equation}
    A_0^2 \equiv \left(1 + \frac{\sigma}{2\, \rho}\right)^{-1} \approx 1 - \frac{\sigma}{2\, \rho} \, ,
\end{equation}
then to first order in $\overline{G}_0\, \tau_\perp^2$ and lowest order in $\sigma$, the four unknown parameters in \eqn{mflt_field_ansatz} are given by
\begin{subequations}
    \begin{align}
        A^2 &\approx A_0^2 - \frac{\sigma^3}{12\, \rho^2}\, \frac{\overline{G}_0\, \tau_\perp^2}{D_2^2}\, ,\\
        a &\approx \frac{\sigma^2}{2\, \rho}\, \frac{\overline{G}_0\, \tau_\perp^2}{D_2^2}\, , \\
        b &\approx \frac{\sigma\, A_0^2}{D_2} + \frac{\sigma^3}{6\, \rho}\, \frac{\overline{G}_0\, \tau_\perp^2}{D_2^3}\, , \nd \\
        \lambda &\approx \frac{\sigma^2\, A_0^4}{6\, D_2} - \frac{\sigma}{2}\, \frac{\overline{G}_0\, \tau_\perp^2}{D_2}\, .
    \end{align}
\end{subequations}