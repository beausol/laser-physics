\begin{subequations}
    \begin{align}
        \rho &\equiv \frac{G_\mathrm{th}}{2\, \Gn} \left( \Gn - G_\mathrm{th} \right)\, , \nd \\
        \sigma &\equiv \frac{\Gn}{2\, \kappa\, G_\mathrm{th}} \left( \Gn - G_\mathrm{th} \right) \left(2 \tau_\parallel + \tau_\perp\right) \ln\left(\frac{1}{\sqrt{R_1\, R_2}}\right)\, .
